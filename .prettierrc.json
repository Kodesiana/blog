{
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "endOfLine": "lf",
  "proseWrap": "always",
  "plugins": ["prettier-plugin-tailwindcss"],
  "goTemplateBracketSpacing": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "go-template",
        "bracketSameLine": true
      }
    }
  ]
}
